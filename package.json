{
  "author": "BRAHIM BOUSSADJRA",
  "name": "vueye-datatable",
  "main": "./dist/vueye-table.common.js",
  "version": "0.1.6",
  "private": false,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
